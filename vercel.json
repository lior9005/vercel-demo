{
    "version": 2,
    "builds": [
      {
        "src": "pages/api/fastApi.py", 
        "use": "@vercel/python"
      },
      {
        "src": "next.config.js",  
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",  
        "dest": "pages/api/fastApi.py"
      },
      {
        "src": "/(.*)",  
        "dest": "/"
      }
    ]
  }
  